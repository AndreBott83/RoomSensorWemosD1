Strategies {
    everyMinute : "0 * * * * ?"
    every5Minutes : "0 */5 * * * ?"
    everyHour   : "0 0 * * * ?"
    everyDay    : "0 0 0 * * ?"
    default = everyChange
}
Items {    
    gWeatherStation* : strategy = everyChange
    gSensor* : strategy = everyChange, restoreOnStartup
    gSensorStatus* : strategy = everyChange, restoreOnStartup
}
